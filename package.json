{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc",
    "dev": "npm run build && tsc-watch  --onSuccess \"nodemon lib/index.js",
    "prod": "npm run build && pm2 start start.json",
    "clean": "node node_modules/rimraf/bin lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.9",
    "@colyseus/monitor": "^0.14.3",
    "@colyseus/social": "^0.10.9",
    "@types/fs-extra": "^9.0.11",
    "@types/serve-index": "^1.7.30",
    "colyseus": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fs-extra": "^9.1.0",
    "log4js": "^6.3.0",
    "mysql2": "^2.2.5",
    "serve-index": "^1.9.1",
    "tsc-watch": "^4.2.9",
    "typed-emitter": "^1.3.1",
    "typeorm": "^0.2.32",
    "winston": "^3.3.3"
  }
}
